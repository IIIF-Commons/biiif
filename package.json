{
  "name": "biiif",
  "version": "1.0.0",
  "description": "A CLI to build IIIF collections",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edsilv/biiif.git"
  },
  "scripts": {
    "build": "npm run format && tsc",
    "format": "prettier --write \"**/*.*\"",
    "test": "mocha",
    "testbuild": "node --nolazy --inspect-brk=5858 -e \"require('./index').build('test/collection', 'https://sitename.netlify.app')\"",
    "serve": "serve test"
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=3.10.8"
  },
  "keywords": [
    "IIIF",
    "nodejs",
    "CLI"
  ],
  "author": "@edsilv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edsilv/biiif/issues"
  },
  "homepage": "https://github.com/edsilv/biiif#readme",
  "dependencies": {
    "@iiif/vocabulary": "^1.0.20",
    "chalk": "^4.1.0",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^4.0.1",
    "is-url": "^1.2.4",
    "js-yaml": "^4.0.0",
    "jsonfile": "^6.1.0",
    "sharp": "^0.28.0",
    "url-join": "^2.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.27",
    "mocha": "8.3.0",
    "mock-fs": "4.13.0",
    "prettier": "^2.2.1",
    "prettier-check": "^2.0.0",
    "serve": "^11.3.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.1.5",
    "typescript-tslint-plugin": "^1.0.1"
  }
}
