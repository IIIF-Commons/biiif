{
    "settings": {
        "jimpEnabled": true
    },
    "thumbnails": {
        "width": 100,
        "height": 100
    },
    "annotation": {
        "motivations": {
            "painting": {
                ".crt": [
                    {
                        "type": "PhysicalObject",
                        "format": "application/corto"
                    }
                ],
                ".drc": [
                    {
                        "type": "PhysicalObject",
                        "format": "application/draco"
                    }
                ],
                ".glb": [
                    {
                        "type": "PhysicalObject",
                        "format": "model/gltf-binary"
                    }
                ],
                ".gltf": [
                    {
                        "type": "PhysicalObject",
                        "format": "model/gltf+json"
                    }
                ],
                ".gz": [
                    {
                        "type": "PhysicalObject",
                        "format": "application/gzip"
                    }
                ],
                ".stl": [
                    {
                        "type": "PhysicalObject",
                        "format": "model/stl"
                    }
                ],
                ".jpg": [
                    {
                        "type": "Image",
                        "format": "image/jpeg"
                    }
                ],
                ".jpeg": [
                    {
                        "type": "Image",
                        "format": "image/jpeg"
                    }
                ],
                ".json": [
                    {
                        "type": "Text",
                        "format": "application/json"
                    },
                    {
                        "type": "PhysicalObject",
                        "format": "application/vnd.threejs+json"
                    },
                    {
                        "type": "Image",
                        "format": "image/jpeg"
                    }
                ],
                ".mp3": [
                    {
                        "type": "Sound",
                        "format": "audio/mp3"
                    }
                ],
                ".mp4": [
                    {
                        "type": "Video",
                        "format": "video/mp4"
                    }
                ],
                ".nii": [
                    {
                        "type": "PhysicalObject",
                        "format": "application/octet-stream"
                    }
                ],
                ".obj": [
                    {
                        "type": "PhysicalObject",
                        "format": "text/plain"
                    }
                ],
                ".pdf": [
                    {
                        "type": "PDF",
                        "format": "application/pdf"
                    }
                ],
                ".ply": [
                    {
                        "type": "PhysicalObject",
                        "format": "application/ply"
                    }
                ],
                ".png": [
                    {
                        "type": "Image",
                        "format": "image/png"
                    }
                ]
            },
            "commenting": {
                ".txt": [
                    {
                        "type": "TextualBody",
                        "format": "text/plain"
                    }
                ]
            }
        }
    }
}